<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ryan.snakegame" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.ryan.snakegame.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.ryan.snakegame.Game"
            android:label="@string/title_activity_game" >
        </activity>
        <activity
            android:name="com.example.ryan.snakegame.BounceGame"
            android:label="@string/title_activity_BounceGame" >
        </activity>

    </application>

</manifest>
